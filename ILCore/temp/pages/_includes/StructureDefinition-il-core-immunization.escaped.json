ate&quot;,&quot;definition&quot;:&quot;Date the educational material was published.&quot;,&quot;min&quot;:0,&quot;max&quot;:&quot;1&quot;,&quot;base&quot;:{&quot;path&quot;:&quot;Immunization.education.publicationDate&quot;,&quot;min&quot;:0,&quot;max&quot;:&quot;1&quot;},&quot;type&quot;:[{&quot;code&quot;:&quot;dateTime&quot;}],&quot;constraint&quot;:[{&quot;key&quot;:&quot;ele-1&quot;,&quot;severity&quot;:&quot;error&quot;,&quot;human&quot;:&quot;All FHIR elements must have a @value or children&quot;,&quot;expression&quot;:&quot;hasValue() or (children().count() &gt; id.count())&quot;,&quot;xpath&quot;:&quot;@value|f:*|h:div&quot;,&quot;source&quot;:&quot;http://hl7.org/fhir/StructureDefinition/Element&quot;}],&quot;isModifier&quot;:false,&quot;isSummary&quot;:false,&quot;mapping&quot;:[{&quot;identity&quot;:&quot;v2&quot;,&quot;map&quot;:&quot;OBX-5 : OBX-3 = 29768-9&quot;},{&quot;identity&quot;:&quot;rim&quot;,&quot;map&quot;:&quot;N/A&quot;}]},{&quot;id&quot;:&quot;Immunization.education.presentationDate&quot;,&quot;path&quot;:&quot;Immunization.education.presentationDate&quot;,&quot;short&quot;:&quot;Educational material presentation date&quot;,&quot;definition&quot;:&quot;Date the educational material was given to the patient.&quot;,&quot;min&quot;:0,&quot;max&quot;:&quot;1&quot;,&quot;base&quot;:{&quot;path&quot;:&quot;Immunization.education.presentationDate&quot;,&quot;min&quot;:0,&quot;max&quot;:&quot;1&quot;},&quot;type&quot;:[{&quot;code&quot;:&quot;dateTime&quot;}],&quot;constraint&quot;:[{&quot;key&quot;:&quot;ele-1&quot;,&quot;severity&quot;:&quot;error&quot;,&quot;human&quot;:&quot;All FHIR elements must have a @value or children&quot;,&quot;expression&quot;:&quot;hasValue() or (children().count() &gt; id.count())&quot;,&quot;xpath&quot;:&quot;@value|f:*|h:div&quot;,&quot;source&quot;:&quot;http://hl7.org/fhir/StructureDefinition/Element&quot;}],&quot;isModifier&quot;:false,&quot;isSummary&quot;:false,&quot;mapping&quot;:[{&quot;identity&quot;:&quot